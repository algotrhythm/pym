
"""AST list handling

AS Trees are usually lists of lists
    If items in a list are named then the list might be a dict

Facilities to handle such lists
"""
import sys

import pudb

from pym.lists import attribute_comparison
from pym.lists import Line
from pym.lists import _search


def main():
    line_compare = attribute_comparison('line', 44)
    lines = [Line(i, l) for i, l in enumerate(
        file(__file__).read().splitlines(), 1)]
    pudb.set_trace()
    return _search(lines, line_compare)


if __name__ == '__main__':
    sys.exit(main())
